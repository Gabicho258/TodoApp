e(tarea);\r\n  };\r\n\r\n  // funcion para editar una tarea\r\n  const handleEditar = async ({ id, title }) => {\r\n    await dispatch(updateTodoAsync({ id, title }));\r\n    dispatch(getTodosByUserAsync(user.id));\r\n\r\n    setEditable(null);\r\n  };\r\n\r\n  // Eliminar una tarea\r\n  const handleEliminar = async (id) => {\r\n    await dispatch(deleteTodoAsync(id));\r\n    dispatch(getTodosByUserAsync(user.id));\r\n  };\r\n\r\n  // Renderizar el componente\r\n  return (\r\n    <>\r\n      <div className=\"container\">\r\n        <h1 className=\"text-center mt-5 mb-5\">\r\n          Lista de tareas - {user?.name}\r\n        </h1>\r\n        <Formulario\r\n          handleRegistrar={handleRegistrar}\r\n          handleEditar={handleEditar}\r\n          editable={editable}\r\n        />\r\n        <ListaTareas\r\n          // listaTareas={listaTareas}\r\n          todos={todos}\r\n          handleToggle={handleToggle}\r\n          handleEliminar={handleEliminar}\r\n          recibirEditable={recibirEditable}\r\n        />\r\n        <Button\r\n          variant=\"danger\"\r\n          type=\"button\"\r\n          onClick={() => {\r\n            localStorage.removeItem(\"user\");\r\n            window.location = \"/\";\r\n          }}\r\n          className=\"mt-2 mb-2\"\r\n        >\r\n          Cerrar sesión\r\n        </Button>\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n","G:\\Proyects React\\workshop-react\\src\\components\\ListaTareas.js",[],"G:\\Proyects React\\workshop-react\\src\\hooks\\useLocalStorage.js",[],"G:\\Proyects React\\workshop-react\\src\\utils\\tareas.js",[],["79","80"],"G:\\Proyects React\\workshop-react\\src\\components\\Formulario.js",[],"G:\\Proyects React\\workshop-react\\src\\helpers\\tareasReducer.js",[],"G:\\Proyects React\\workshop-react\\src\\App.js",[],"G:\\Proyects React\\workshop-react\\src\\pages\\Register.js",["81"],"import React, { useEffect } from \"react\";\r\nimport { Button, Form } from \"react-bootstrap\";\r\nimport { useDisp